events {
    worker_connections 1024;
}

http {
    upstream backend {
        server nodeServer:8210;
    }
    server {
        listen 80;

        location / {
            auth_basic "Restricted Access";
            auth_basic_user_file /etc/nginx/.htpasswd;
            root /usr/share/nginx/html;
            index index.html;
        }
    }
}